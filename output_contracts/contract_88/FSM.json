{
    "contractName": "NCO",
    "inherited contracts": ["ERC20Interface", "Owned", "SafeMath"],
    "initialState": "Initialized",
    "states": [
        {
            "name": "Initialized",
            "transitions": [
                {
                    "trigger": "Transfer",
                    "target": "TransferInProgress",
                    "action": "initiateTransfer"
                }
            ]
        },
        {
            "name": "TransferInProgress",
            "transitions": [
                {
                    "trigger": "TransferComplete",
                    "target": "TransferCompleted",
                    "action": "completeTransfer"
                },
                {
                    "trigger": "TransferFailed",
                    "target": "TransferFailedState",
                    "action": "handleTransferFailure"
                }
            ]
        },
        {
            "name": "TransferCompleted",
            "transitions": [
                {
                    "trigger": "NewTransfer",
                    "target": "TransferInProgress",
                    "action": "initiateTransfer"
                }
            ]
        },
        {
            "name": "TransferFailedState",
            "transitions": [
                {
                    "trigger": "RetryTransfer",
                    "target": "TransferInProgress",
                    "action": "retryTransfer"
                }
            ]
        }
    ],
    "variables": [
        {
            "name": "symbol",
            "type": "string",
            "initialValue": "NCO"
        },
        {
            "name": "name",
            "type": "string",
            "initialValue": "NoneCoin"
        },
        {
            "name": "decimals",
            "type": "uint8",
            "initialValue": 18
        },
        {
            "name": "_totalSupply",
            "type": "uint",
            "initialValue": 30000000000000000000000000
        },
        {
            "name": "balances",
            "type": "mapping(address => uint)",
            "initialValue": {}
        },
        {
            "name": "allowed",
            "type": "mapping(address => mapping(address => uint))",
            "initialValue": {}
        }
    ],
    "functions": [
        {
            "name": "totalSupply",
            "function": "Returns the total supply of tokens."
        },
        {
            "name": "balanceOf",
            "function": "Returns the token balance for a given address."
        },
        {
            "name": "transfer",
            "function": "Transfers tokens from the sender to a specified address."
        },
        {
            "name": "approve",
            "function": "Allows a spender to withdraw from the owner's account."
        },
        {
            "name": "transferFrom",
            "function": "Transfers tokens from one address to another using allowance."
        },
        {
            "name": "allowance",
            "function": "Returns the amount of tokens approved for transfer."
        },
        {
            "name": "approveAndCall",
            "function": "Approves a spender and calls a function on the spender."
        },
        {
            "name": "transferAnyERC20Token",
            "function": "Allows the owner to transfer out any accidentally sent ERC20 tokens."
        }
    ],
    "events": ["Transfer", "Approval"]
}
