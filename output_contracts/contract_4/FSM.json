            "initialValue": "msg.sender"
        },
        {
            "name": "rentrancyLock",
            "type": "bool",
            "initialValue": false
        }
    ],
    "functions": [
        {
            "name": "addLiquidity",
            "function": "The function to add liquidity to the contract."
        },
        {
            "name": "removeLiquidity",
            "function": "The function to remove liquidity from the contract."
        },
        {
